<?xml version="1.0" encoding="UTF-8"?><plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    id="com.phonegap.plugins.will"
    version="1.0.0">

    <name>will sdk</name>
    <description></description>
    <license></license>

    <repo></repo>
    <issue></issue> 
    
    <engines>
        <engine name="cordova" version=">=3.0.0" />
    </engines>    

    <js-module src="www/will.js" name="will">
        <clobbers target="cordova.plugins.will" />
    </js-module>

	  <platform name="android">

    <config-file target="res/xml/config.xml" parent="/*">
      <feature name="willplugin">
        <param name="android-package" value="com.phonegap.plugins.will.willplugin" />
      </feature>
    </config-file>

    <source-file src="src/android/com/phonegap/plugins/will/willplugin.java" target-dir="src/com/phonegap/plugins/will" />

  </platform>




</plugin>
